#include<bits/stdc++.h>
using namespace std;

int main()
{
	string str;
	cin>>str;
	string s;
	int p=0;
	for(int i=0;i<5;i++)
	{
		cin>>s;
		if(s[0]==str[0] || s[1]==str[1])
		{
			cout<<"YES";
			return 0;
		}
	}
	cout<<"NO";

	return 0;

}
